    "babel-loader": "^9.1.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "jest-enzyme": "^7.1.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
