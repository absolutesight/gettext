  "homepage": "https://github.com/eugene-manuilov/react-gettext#readme",
  "bugs": {
    "url": "https://github.com/eugene-manuilov/react-gettext/issues"
  },
  "version": "1.0.2",
  "main": "lib/index.js",
  "files": [
    "*.md",
    "dist",
    "LICENSE",
